extends layout.pug

block content 
  nav.grid-container.portfolio-breadcrumb
    a(href='/') ‚Üê Back
  article.grid-container.portfolio-intro.portfolio-project
    .grid-x
      .cell.small-6.medium-9.large-9

        h1 #{templateData.project_name}

        p.lead.text-light
          | #{templateData.description}

      .cell.portfolio-meta.small-6.medium-2.large-3
        h6 Technologies

        ul
          each tech in templateData.technologies
            li #{tech}

        a.button.expanded(href=`${templateData.live_link}`, target='_blank') Live Demo
        a.button.expanded(href=`${templateData.github_link}`, target='_blank') GitHub Repo
    
    img.image(src=`${templateData.image_urls}`)
